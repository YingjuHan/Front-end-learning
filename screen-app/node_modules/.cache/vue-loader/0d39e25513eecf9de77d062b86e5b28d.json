 from \"vuex\";\r\n\r\nexport default defineComponent({\r\n    name: \"operation-screen\",\r\n    components: {\r\n        TimeTabs,\r\n        ScreenHeader,\r\n    },\r\n    setup() {\r\n        let options = reactive({\r\n            titleText: \"公交大数据运营数据分析\",\r\n            layout: [\r\n                {\r\n                    title: \"线网\",\r\n                    name: \"line-net\",\r\n                },\r\n                {\r\n                    title: \"车辆\",\r\n                    name: \"bus\",\r\n                },\r\n                {\r\n                    title: \"违规原因分析\",\r\n                    name: \"getout-reasons\",\r\n                },\r\n                {\r\n                    title: `卡类型使用情况`,\r\n                    name: \"card-type-use\",\r\n                },\r\n                {\r\n                    title: \"区域客流统计\",\r\n                    name: \"area-passenger-stat\",\r\n                }, //地图\r\n                {\r\n                    title: `线路运客数排名`,\r\n                    name: \"transport-passenger-rank\",\r\n                },\r\n                {\r\n                    title: `电子支付趋势`,\r\n                    name: \"e-pay-trend\",\r\n                },\r\n            ],\r\n        });\r\n        const dataArray = markRaw([\r\n            { label: \"日\", value: 0 },\r\n            { label: \"周\", value: 1 },\r\n            { label: \"月\", value: 2 },\r\n            { label: \"年\", value: 3 },\r\n        ]);\r\n        const store = useStore();\r\n        const screenDataType = computed(() => {\r\n            return store.getters[\"app-global/GET_DATATYPE\"];\r\n        });\r\n        let curType = ref(screenDataType.value);\r\n        function onChange(val: string | number) {\r\n            curType.value = val;\r\n            store.commit(\"app-global/GET_DATATYPE\", val);\r\n        }\r\n        return {\r\n            ...toRefs(options),\r\n            dataArray,\r\n            curType,\r\n            onChange,\r\n        };\r\n    },\r\n});\r\n</script>\r\n<!-- \r\n<style lang=\"scss\" scoped>\r\n@import \"./index.scss\";\r\n.operation-screen {\r\n  background-image: url(\"/imgs/bg/operate-bg.png\");\r\n  background-size: cover;\r\n  background-repeat: no-repeat;\r\n  height: 100%;\r\n  width: 100%;\r\n  position: absolute;\r\n}\r\n.time-tab-box {\r\n    position: absolute;\r\n    right: 20px;\r\n    top: 40px;\r\n    z-index: 9;\r\n}\r\n</style> -->\r\n"]}]}